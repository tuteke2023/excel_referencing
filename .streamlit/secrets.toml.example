# Streamlit Cloud Secrets Configuration
# Copy this to Settings > Secrets in Streamlit Cloud

# Claude Code API Relay (for AI-powered analysis)
CLAUDE_API_URL = "https://claude-api.ttaccountancy.au"
CLAUDE_API_TOKEN = "your-api-token-here"

# Cloudflare Access Service Token (if API is protected by Cloudflare Access)
# Create a service token at: Cloudflare Dashboard > Zero Trust > Access > Service Auth
CF_ACCESS_CLIENT_ID = "your-service-token-client-id.access"
CF_ACCESS_CLIENT_SECRET = "your-service-token-client-secret"
